<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.runningfish.spmk.service.config">
    <select id="getArgumentsValue" parameterType="string" resultType="string">
        select argumentsValue 
          from t_arguments
          where argumentsName=#{value}
    </select>
</mapper>